@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="app">
<head>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Script/app.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <section ng-controller="EventosController">
        <table>
            <tr>
                <th ng-repeat="dado in dados">
                    {{()dado.nome}}
                </th>
                <th>
                    {{dado.data}}
                </th>
            </tr>

            <tr ng-repeat="item in dados">
                <td ng-repeat="(Key,value) in item">{{value}}</td>
            </tr>
        </table>
    </section>
</body>
</html>
